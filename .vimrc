syntax on 
set tabstop=4
set shiftwidth=4
set softtabstop=4
set ruler
set number
map Y y$
set wildmenu
set showcmd
set smartcase
set mouse=a
set virtualedit=onemore 
set history=1000
set spell
set cursorline  
hi cursorline guibg=#333333
hi CursorColumn guibg=#333333  
set backspace=indent,eol,start
set showmatch 
set incsearch
set scrolloff=3
set autoindent	
cmap W w
cmap X x
